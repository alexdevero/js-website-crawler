{
  "name": "js-website-crawler",
  "version": "1.0.0",
  "description": "Website crawler written in JavaScript built with Node.js.",
  "main": "crawler.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=0.27.x"
  },
  "scripts": {
    "start": "set NODE_ENV=development && node ./src/crawler.js",
    "build": "set NODE_ENV=production && rimraf ./dist && babel ./src/crawler.js --out-file ./dist/crawler.js",
    "test": "set NODE_ENV=test && eslint ./src/crawler.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdevero/js-website-crawler.git"
  },
  "keywords": [
    "crawler",
    "ES6",
    "ES7",
    "javascript"
  ],
  "author": "Alex Devero <deveroalex@gmail.com> (http://github.com/alexdevero)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexdevero/js-website-crawler/issues"
  },
  "homepage": "",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "readline": "^1.3.0",
    "request": "^2.83.0",
    "url-parse": "^1.1.9"
  },
  "devDependencies": {
    "eslint": "^4.8.0"
  }
}
